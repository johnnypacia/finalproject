<div class ="banner">
	<h1>Sign up For Sound Shaping Today!</h1>
</div>
<div id="welcome-wagon">
	<ul class="login grid-6-8">
		<%= form_for @user, :url => users_path do |f|%>
			<li><%=f.label :first_name%></li>
			<li><%=f.text_field :first_name%></li>
			<li><%=f.label :last_name%></li>
			<li><%=f.text_field :last_name%></li>
			<li><%=f.label :email%></li>
			<li><%=f.text_field :email%></li>
			<li><%=f.label :password%></li>
			<li><%=f.password_field :password%></li>
			<li><%=f.label :password_confirmation%></li>
			<li><%=f.password_field :password_confirmation%></li>
			<li><%=f.submit "Submit"%></li>
		<%end%>
	</ul>
</div>

<a href="/"> Back to Sign In</a>